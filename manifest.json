{
    "manifest_version": 2,
    "name": "Foco Cpanel Bot",
    "version": "1.0.0",
    "description": "Crie zonas master no Cpanel da Foco Multim√≠dia com poucos cliques!",
    "icons": {
       "16": "./src/assets/icon16.png",
       "48": "./src/assets/icon48.png",
       "128": "./src/assets/icon128.png"
    },
    "web_accessible_resources": [
        "src/assets/*",
        "src/script.js",
        "src/Bot/index.js",
        "src/Bot/Login.js",
        "src/Bot/Navigation.js",
        "src/Bot/Avatar.js",
        "src/Bot/Menu.js",
        "src/Bot/MasterZone.js",
        "src/Bot/Storage.js"
    ],
    "page_action": {
        "default_title": "Foco Cpanel Bot"
    },
    "content_scripts": [
        {
            "matches": [
                "https://sdn01fmu.focomultimidia.com:10000/*"
            ],
            "css": [
                "./src/style.css"
            ],
            "js": [
                "./src/enableES6.js"
            ]
        }
    ]
}